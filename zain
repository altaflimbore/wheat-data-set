#include<stdio.h>
#include<sys/types.h>
#include<unistd.h>

int main()
{
    pid_t p;
    p=fork();

    if(p<0)
    {
        printf("Error\n");
    }
    else if(p==0)
    {
        printf("child process having pid %d\n",getpid());
        printf("my parents id id %d\n",getppid());
    }
    else
    {
        printf("parent process having id %d\n",getpid());
        printf("my child's process is %d\n",p);
    }
    return 0;
}
